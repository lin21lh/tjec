#parse("common/extensions-v14.vm")
<link href="$contextpath/component/uploadify/uploadify.css" rel="stylesheet" type="text/css" /> 
<script type="text/javascript" src="$contextpath/component/uploadify/jquery.uploadify-3.1.min.js"></script>	
<script type="text/javascript" src="$contextpath/templates/ppms/common/common.js"></script>
<style type='text/css'>
input {
	line-height:20px;
	border:1px solid #95B8E7;
}
</style>
<form id="transferAddForm" method="post">
	<table class="list">
    		<!--隐藏属性start
    			-->
			<input type="hidden" id="workflowflag" name="workflowflag" /><!--区分保存还是保存并发送-->
			<input type="hidden" id="projectid" name="projectid" /><!--项目id-->
			<input type="hidden" id="transferid" name="transferid" /><!--项目移交id-->
			<input type="hidden" id="menuid" name="menuid" /><!--菜单id-->
			<input type="hidden" id="wfid" name="wfid" /><!--工作流id-->
			<input type="hidden" id="activityId" name="activityId" /><!--工作流节点id-->
    		<!--隐藏属性end
    		-->
    		<tr>
    			<th>项目移交准备时间</th>
    			<td>
    				<input id="transferStartTime" class="easyui-datebox" name="transferStartTime" editable ='false' required="true"   missingMessage="请选择项目移交准备时间" style='width:250px'/>
    			</td >
    			<th>项目移交完成时间</th>
    			<td>
    				<input id="transferEndTime" class="easyui-datebox" name="transferEndTime" editable ='false' required="true" missingMessage="请选择移交完成时间" style='width:250px' />
    			</td>
    		</tr>
			<tr>
				<th>资产评估时间</th>
				<td colspan="3">
					<input id="assessTime" class="easyui-datebox" name="assessTime" editable ='false' required="true" missingMessage="请选择资产评估时间" style='width:250px'/>
				</td>
			</tr>	
			<tr>
				<th>资产评估结果</th>
				<td colspan="3">
					<input id="assetAssess" class="easyui-textbox" name="assetAssess" multiline="true" required="true" missingMessage="请输入资产评估结果" data-options="prompt:'最多可输入500个汉字',validType:{length:[0,500]}" style="width:695px;height:50px;" />
				</td>
			</tr>	
				<tr>
				<th>资产评估结果报告</th>
				<td  id="zcpgjg" style="height:31px">
    				<input id="assetAssessPath" type="hidden" name="assetAssessPath"/>
					<img src="component/jquery-easyui-1.4/themes/icons/files.png" style="float:right;cursor:pointer" onClick="uploadFile('zcpgjg','assetAssessPath',30)"/>
				</td>
				<td colspan="2">
				</td>
			</tr>
			<tr>
				<th>项目性能测试结果</th>
				<td colspan="3">
					<input id="transferProperty" class="easyui-textbox" name="transferProperty" multiline="true" required="true" missingMessage="请输入项目性能测试结果" data-options="prompt:'最多可输入500个汉字',validType:{length:[0,500]}" style="width:695px;height:50px;" />
				</td>
			</tr>
			<tr>
				<th>项目性能测试报告</th>
				<td  id="xmxncejg" style="height:31px">
    				<input id="transferPropertyPath" type="hidden" name="transferPropertyPath"/>
					<img src="component/jquery-easyui-1.4/themes/icons/files.png" style="float:right;cursor:pointer" onClick="uploadFile('xmxncejg','transferPropertyPath',30)"/>
				</td>
				<td colspan="2">
				</td>
			</tr>
			<tr>
				<th>项目绩效评价结果</th>
				<td colspan="3">
					<input id="projectPerformance" class="easyui-textbox" name="projectPerformance" multiline="true" required="true" missingMessage="请输入项目绩效评价结果" data-options="prompt:'最多可输入500个汉字',validType:{length:[0,500]}" style="width:695px;height:50px;" />
				</td>
			</tr>
			<tr>
				<th>项目绩效评价报告</th>
				<td  id="xmjxpjbg" style="height:31px">
    				<input id="projectPerformancePath" type="hidden" name="projectPerformancePath"/>
					<img src="component/jquery-easyui-1.4/themes/icons/files.png" style="float:right;cursor:pointer" onClick="uploadFile('xmjxpjbg','projectPerformancePath',30)"/>
				</td>
				<td colspan="2">
				</td>
			</tr>
			<tr>
				<th>备注</th>
				<td colspan="3">
					<input id="remark" class="easyui-textbox" name="remark" multiline="true"  data-options="prompt:'最多可输入500个汉字',validType:{length:[0,500]}" style="width:695px;height:50px;" />
				</td>
			</tr>
    </table>
</form>
<!--上传附件层-->
<div id='uploadifydiv'></div>