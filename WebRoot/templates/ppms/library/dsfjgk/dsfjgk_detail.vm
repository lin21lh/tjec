<script type="text/javascript" src="$contextpath/templates/ppms/common/common.js"></script>
<style type='text/css'>
input {
	line-height:20px;
	border:1px solid #95B8E7;
}
</style>
<form id="dsfjgkDetailForm" method="post">
	<table class="list">
        		<!--隐藏属性start
        			-->
				<input type="hidden" id="dsfjgid" name="dsfjgid" /><!--第三方机构id-->
				
        		<!--隐藏属性end
        		-->
        		<tr>
					<th >机构代码</th>
					<td >
						<input id="organCode" name="organCode" class="easyui-textbox"  style='width:250px'  required="true" readonly ="true"/>
					</td>
        			<th >机构名称</th>
        			<td >
        				<input id="organName" name="organName" class="easyui-textbox"  style='width:250px'  required="true" readonly ="true"/>
					</td>
                </tr>
				<tr>
					<th >委托方</th>
					<td >
						<input id="consignor" name="consignor" class="easyui-textbox"  style='width:250px' readonly ="true" />
					</td>
        			<th >项目经理</th>
        			<td >
        				<input id="projectManager" name="projectManager" class="easyui-textbox"  style='width:250px' readonly ="true"  />
					</td>
                </tr>
				<tr>
					<th >联系电话</th>
					<td >
						<input id="phone" name="phone" class="easyui-textbox"  style='width:250px' readonly ="true" />
					</td>
        			<th >手机号码</th>
        			<td >
        				<input id="mobile" name="mobile" class="easyui-textbox"  style='width:250px' readonly ="true"  />
					</td>
                </tr>
				<tr>
					<th >微信标识</th>
					<td >
						<input id="weixin" name="weixin" class="easyui-textbox"  style='width:250px' readonly ="true" />
					</td>
					<th >是否有效</th>
					<td >
						<input id="sfyxName" name="sfyxName" class="easyui-textbox"  style='width:250px'  readonly ="true"/>
					</td>
                </tr>
				<tr>
        			<th >服务内容</th>
        			<td colspan="3">
        				<input id="content" name="content"  readonly ="true" multiline="true"
							 class="easyui-textbox"   style='width:695px;height:45px;'/>
					</td>
                </tr>
        </table>
		
</form>

