#parse("common/extensions-v14.vm")
<script type="text/javascript">
</script>
<style type='text/css'>
    input {
    line-height:20px;
    border:1px solid #95B8E7;
    }
</style>
<form id="prores_form" method="post">
    <input id="projectid" type="hidden" name='projectid'/>
    <input id="purchaseid" type="hidden" name='purchaseid'/>
    <input id="createuser" type="hidden" name='createuser'/>
    <input id="createtime" type="hidden" name='createtime'/>
    <input id="updateuser" type="hidden" name='updateuser'/>
    <input id="updatetime" type="hidden" name='updatetime'/>
    <input id="wfid" type="hidden" name='wfid'/>
    <input id="status" type="hidden" name='status'/>
    <input id="contractPath" type="hidden" name='contractPath'/>
    <input id="datatype" type="hidden" name='datatype'/>
    <input id="advanceid" type="hidden" name='advanceid'/>
	<input id="subStr" type="hidden" name='subStr'/>
<div id="tabList" class="easyui-tabs">
	<div title="基本信息">
    <table class="list">
		<tr>
            <th>项目名称</th>
            <td>
                <input id="proName" class="easyui-textbox"  missingMessage="请选择项目名称" name="proName" style='width:250px' readonly='true'/>
            </td>
            <th>项目类型</th>
            <td >
                <input id="proTypeName" class="easyui-textbox"  missingMessage="请输入项目类型" name="proTypeName" style='width:250px' readonly='true' />
            </td>
        </tr>
        <tr>
            <td colspan="4"><table id="advance_organ"></table></td>
		</tr>
		<tr>	
             <th>采购公告时间</th>
            <td>
                <input id="purchaseNoticeTime" class="easyui-textbox"  missingMessage="请输入采购公告时间" name="purchaseNoticeTime" style='width:250px' readonly='true' />
            </td>
            <th>公告发布媒体</th>
            <td >
                <input id="purchaseNoticeMedia" class="easyui-textbox"  missingMessage="请输入公告发布媒体" name="purchaseNoticeMedia" style='width:250px'  readonly='true' />
            </td>
        </tr>
		<tr>	
             <th>响应文件提交时间</th>
            <td>
                <input id="fileCommitTime" class="easyui-textbox"  missingMessage="请输入响应文件提交时间" name="fileCommitTime" style='width:250px' readonly='true' />
            </td>
            <th>响应文件评审时间</th>
            <td >
                <input id="fileJudgeTime" class="easyui-textbox"  missingMessage="请输入响应文件评审时间" name="fileJudgeTime" style='width:250px'  readonly='true' />
            </td>
        </tr>
		<tr>	
             <th>结果确认谈判时间</th>
            <td>
                <input id="negotiateTime" class="easyui-textbox"  missingMessage="请输入结果确认谈判时间" name="negotiateTime" style='width:250px' readonly='true' />
            </td>
            <th>政府审核时间</th>
            <td >
                <input id="govVerifyTime" class="easyui-textbox"  missingMessage="请输入政府审核时间" name="govVerifyTime" style='width:250px'  readonly='true' />
            </td>
        </tr>
		<tr>	
             <th>合同签署时间</th>
            <td>
                <input id="contractTime" class="easyui-textbox"  missingMessage="请输入合同签署时间" name="contractTime" style='width:250px' readonly='true' />
            </td>
            <th>合同公布时间</th>
            <td >
                <input id="contractPublishTime" class="easyui-textbox"  missingMessage="请输入合同公布时间" name="contractPublishTime" style='width:250px'  readonly='true' />
            </td>
        </tr>
        <tr>	
             <th>合同公布媒体</th>
            <td >
                <input id="contractPublishMedia" class="easyui-textbox"  missingMessage="请输入合同公布媒体" name="contractPublishMedia" style='width:250px' readonly='true' />
            </td>
            <th>ppp项目合同附件</th>
            <td  colspan="3" id='prorespath'>
                <input id="contractPath" type="hidden"  name="contractPath" />
			</td>
        </tr>
		<tr>
            <th>备注</th>
            <td colspan="3">
                <input id="remark" class="easyui-textbox"  multiline="true" missingMessage="请输入备注" name="remark" style='width:670px;height:50px;'  readonly='true' />
            </td>
        </tr>
        
    </table>
	 </div>
	<div title="专家信息以及确认谈判工作组">
		<table id="resultExpert"><input type="hidden" id="resultExpertData" name="resultExpertData" /></table>
		<table id="resultGroup"><input type="hidden" id="resultGroupData" name="resultGroupData" /></table>
    </div>
</div>
</form>